{
  "plugins": ["prettier"],
  "rules": {
    "no-unused-vars": "error",
    "prefer-const": "error",
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "trailingComma": "all"
      }
    ]
  },
  "parserOptions": { "ecmaVersion": 2017 },
  "env": {
    "node": true,
    "es6": true
  },
  "extends": ["prettier"]
}
